<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd" version="1.2">
	<webservice-description>
		<webservice-description-name>StudentWebService</webservice-description-name>
		<port-component>
			<port-component-name>StudentWebServiceDelegate</port-component-name>
			<wsdl-service xmlns:pfx="http://service.student.demo.com/">
			pfx:StudentWebService</wsdl-service>
			<wsdl-port xmlns:pfx="http://service.student.demo.com/">
			pfx:StudentWebServicePort</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<service-impl-bean>
				<servlet-link>
				com.demo.webservice.delegate.StudentWebServiceDelegate</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
